{
  "name": "WebDeveloper",
  "executor": "local",
  "image": "web_developer",
  "apiVersion": "v1",
  "actions": [
    {
      "name": "DiagnoseBug",
      "description": "Perform a root-cause analysis of a bug based on a description and context.",
      "inputs": [
        {
          "type": "text",
          "name": "bug_description"
        },
        {
          "type": "text",
          "name": "error_context"
        }
      ],
      "outputs": [
        {
          "type": "text",
          "name": "bug_diagnosis"
        }
      ]
    },
    {
      "name": "WriteCode",
      "description": "Write code to implement a feature based on a requirements document",
      "inputs": [
        {
          "type": "text",
          "name": "requirements_document"
        }
      ],
      "outputs": [
        {
          "type": "code",
          "name": "code"
        }
      ]
    }
  ]
}