{
  "name": "ImplementFeature",
  "apiVersion": "v1",
  "description": "Implement a feature based on customer requirements.",
  "team": "RosterTeam",
  "inputs": [
    {
      "type": "text",
      "name": "change_request"
    },
    {
      "type": "text",
      "name": "codebase_tree"
    }
  ],
  "outputs": [
    {
      "type": "code",
      "name": "output_code"
    }
  ],
  "steps": {
    "PlanCode": {
      "role": "WebDeveloper",
      "action": "PlanCodeChanges",
      "inputMap": {
        "change_request": "workflow.change_request",
        "codebase_tree": "workflow.codebase_tree"
      }
    },
    "WriteCode": {
      "role": "WebDeveloper",
      "action": "WriteCode",
      "inputMap": {
        "implementation_plan": "PlanCode.implementation_plan"
      }
    },
    "RefineCode": {
      "role": "WebDeveloper",
      "action": "RefineCode",
      "inputMap": {
        "code": "WriteCode.code",
        "change_request": "workflow.change_request",
        "implementation_plan": "PlanCode.implementation_plan"
      },
      "outputMap": {
        "refined_code": "output_code"
      }
    }
  }
}